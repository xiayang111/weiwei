  		  	android:layout_marginRight="@dimen/dp_default_2"
  		    android:src="@drawable/address_guide_top" 
  		   />

	<ImageView 
	     android:src="@drawable/address_guide_bottom"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    
	    android:layout_marginLeft="@dimen/dp_default_60"
	
	    
	    
	   /></LinearLayout>
	
</LinearLayout>
